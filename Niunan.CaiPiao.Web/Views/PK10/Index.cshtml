    //初始化玩法奖金
    function initJiangjin() {
        var sp = getWanFaInfo();
        var beishu = 0;
        beishu = $("#beishu").val();
        if (sp.length == 3) {
            var jj = 0.0;
            console.log("sp: " + sp);
            if (sp[2] == "1") {
                var currentMoshi = 0;
                currentMoshi = parseInt($("#Hd_CurrentMoshi").val());
                jj = FormatNumber(currentMoshi * parseFloat(sp[1]) * parseFloat(beishu) * (curMS / 2), 2);
                //   console.log("currentMoshi:,  " + currentMoshi + "sp[1]: " + sp[1] + ", beishu: " + beishu + ",curMS:" + curMS + " , JJ:" + jj);
            }
            else {
                jj = FormatNumber(parseFloat(sp[1]) * parseInt(beishu) * (curMS / 2), 2);
                //  console.log("玩法奖金基数不为1： " + "sp[1]: " + sp[1] + ", beishu: " + beishu + ",curMS:" + curMS + " , JJ:" + jj);
            }
            $("#div_jiangjin").text(jj);
        }
        else {
            $("#div_jiangjin").text("0.00");
        }
    }

    //获取玩法信息,返回数组：玩法ID,奖金基数,模式是否可调 , wanfa表中的数据
    function getWanFaInfo() {
        var wfhtml = $("#Hd_wanfa").val();
        var currentWfid = $("#wfid").val();
        eval("var reg=/\\[" + currentWfid + "\\]_\\d+\\.\\d+_\\d{1}/");
        var rs = wfhtml.match(reg);
        var sp = new Array(3);
        if (rs != null) {
            sp = rs[0].split('_');
        }
        return sp;
    }



    /*点击一级玩法*/
    $('.play-groups .list .item').click(function () {
        var index = $(this).index();
        $('.play-groups .list .item').removeClass('selected');
        $(this).addClass('selected');

        $('.play-list .row').hide();
        $('.play-list .row').eq(index).show();
        //二级玩法第一个选择中
        $('.play-list .row').eq(index).find(".column").removeClass("selected");
        $('.play-list .row').eq(index).find(".column:eq(0)").addClass("selected");
        //相应的玩法区域显示出来
        $('.play-area').hide();
        var arr = $('.play-list .row').eq(index).find(".column:eq(0)").attr("title").split('|');
        var wfid = arr[0];
        var position = arr[1];
        $('#xiazhu-' + wfid).show();
        //设置玩法ID隐藏域
        $('#wfid').val(wfid);
        $('#position').val(position);

        initJiangjin();
    })

    /*点击二级玩法*/
    $('.play-list .row .column').click(function () {
        $(this).parent().find(".column").removeClass("selected");
        $(this).addClass("selected");


        $('.play-area').hide();
        var arr = $(this).attr("title").split('|');
        var wfid = arr[0];
        var position = arr[1];

