    /*点下注号切换高亮状态*/
    $(".balls .item").click(function () {
        var v = $(this).attr("class");
        if (v.indexOf('selected') > -1) {
            $(this).removeClass('selected');
        } else {
            $(this).addClass('selected');
        }
    })

    /*全大小单双清*/
    $('.tools .item').click(function () {
        var txt = $(this).text();
        switch (txt) {
            case "全":
                $(this).parent().prev('.balls').find(".item").addClass('selected');
                break;
            case "大":
                $(this).parent().prev('.balls').find(".item").each(function (i) {
                    var val = $(this).text();
                    if (parseInt(val) >= 5) { $(this).addClass('selected') }
                });
                break;
            case "小":
                $(this).parent().prev('.balls').find(".item").each(function (i) {
                    var val = $(this).text();
                    if (parseInt(val) < 5) { $(this).addClass('selected') }
                });
                break;
            case "单":
                $(this).parent().prev('.balls').find(".item:odd").addClass('selected');
                break;
            case "双":
                $(this).parent().prev('.balls').find(".item:even").addClass('selected');
                break;
            case "清":
                $(this).parent().prev('.balls').find(".item").removeClass('selected');
                break;
        }
    })



    /*取弹出来的内容HTML*/
    function GetPopHtml() {
        var totalprice = 0; //总金额
        var html = '<table class="pop_niunantab">';
        $('.lottery-record .list .item').each(function () {
            var one_xiazhuhao = $.trim($(this).find('.text').text());
            var one_zhushu = $(this).find('.nums').text();
            var one_je = $(this).find('.money').text().replace('元', '')
            totalprice += parseFloat(one_je);
            html += '<tr><td>' + one_xiazhuhao + '</td><td>' + one_zhushu + '</td><td>' + one_je + '元</td></tr>';
        })
        html += '<tr><td>总金额：</td><td></td><td>' + totalprice + '元</td></tr>';
        html += '</table>';
        return html;
    }


