    function get_djs() {
        var endtime = $('#hfendtime').val();
        var url = "/Home/CalcDJS?endtime=" + endtime + "&t=" + new Date().valueOf();
        $.getJSON(url, function (json) {
            $('#hh').html(json.hh);
            $('#mm').html(json.mm);
            $('#ss').html(json.ss);

            //都是0了以后再重新取当前期和上一期
            if (parseInt(json.hh) == 0 && parseInt(json.mm) == 0 && parseInt(json.ss) < 0) {
                var url2 = "/pk10/GetCurrentAndPrev?t=" + new Date().valueOf();
                $.getJSON(url2, function (json2) {
                    $('#span_currentqihao').html(json2.current_qihao);
                    $('#hfendtime').val(json2.current_endtime);
                    $('#span_prevqihao').html(json2.prev_qihao);
